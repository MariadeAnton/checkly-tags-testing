  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@types/lodash": "^4.14.194",
    "@types/uuid": "^10.0.0",
    "playwright-ctrf-json-reporter": "^0.0.14",
    "typescript": "latest",
    "xo": "^0.54.1",
    "checkly": "latest",
    "ts-node": "latest"
  },
  "xo": {
    "rules": {
      "@typescript-eslint/object-curly-spacing": [
        "error",
        "always"
      ],
      "import/extensions": [
        "error",
        "never"
      ],
      "unicorn/filename-case": [
